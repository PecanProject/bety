<h2>Associated Files</h2>


<table>
  <tr>
    <th>Machine</th>
    <th>File Path</th>
    <th>File Name</th>
  </tr>
  <% @model.files.all(:limit => 100).each do |c| %>
  <tr>
    <td><%=h link_to_if c.machine, c.machine, c.machine %></td>
    <td><%=h c.file_path %></td>
    <td><%=h c.file_name %></td>
    <td><%= link_to "Show", dbfile_path(c) %></td>
  </tr>
  <% end %>
</table>
